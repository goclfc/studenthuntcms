{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "company": {
      "type": "string"
    },
    "productName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "logo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "coverUrl": {
      "type": "string"
    },
    "logoUrl": {
      "type": "string"
    },
    "fullDescription": {
      "type": "text"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "available": {
      "type": "integer"
    },
    "sales": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categorie.categorie",
      "mappedBy": "products"
    }
  }
}
